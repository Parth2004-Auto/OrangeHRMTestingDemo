<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OrangeHRMDemo Automation Suite" parallel="tests" thread-count="2">
    
    <listeners>
        <listener class-name="com.orangehrmdemo.reports.TestListener"/>
    </listeners>

    <test name="End to End Tests">
        <classes>
            <class name="com.orangehrmdemo.tests.LoginTest"/>
            <class name="com.orangehrmdemo.tests.DashboardTest"/>
            <class name="com.orangehrmdemo.tests.AdminTest"/>
            <class name="com.orangehrmdemo.tests.PIMTest"/>
            <class name="com.orangehrmdemo.tests.LeaveTest"/>
            <class name="com.orangehrmdemo.tests.MyInfoTest"/>
            <class name="com.orangehrmdemo.tests.DirectoryTest"/>
        </classes>
    </test>
    
</suite>
